//c program for nested for loop 
 #include<stdio.h> 
 int main() 
 { 
 { 
     int i,n; 
     printf("Enter n:"); 
     scanf("%d",&n); 
     for(i=1;i<=5;i++) 
       printf("%d\t",i); 
       printf("\n"); 
     for(i=6;i<=10;i++) 
       printf("%d\t",i); 
       printf("\n"); 
     for(i=11;i<=15;i++) 
       printf("%d\t",i); 
       printf("\n"); 
 } 
 }
